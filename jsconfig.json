{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@api/*": [
        "./api/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@ui/*": [
        "./components/ui/*"
      ],
      "@handlers/*": [
        "./handlers/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@routes/*": [
        "./routes/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@utils/*": [
        "./utils/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}